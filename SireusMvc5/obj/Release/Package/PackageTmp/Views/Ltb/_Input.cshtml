<table style="width: 100%">
    <tr>
        <td style="font-size: small; font-weight: 700; width: 28%;" align="right">
            @Html.Raw("<strong>Konfidensnivå:</strong>&nbsp;")
        </td>
        <td style="font-size: small; font-weight: 700; width: 10%;" align="left">
            @Html.DropDownList("ConfidenceLevels")
        </td>
    </tr>
    <tr>
        <td style="font-size: small; font-weight: 700; width: 28%;" align="right">
            @Html.Raw("<strong>Repair Lead Time [2 - 365]:</strong>&nbsp;")
        </td>
        <td style="font-size: small; font-weight: 700; width: 10%;" align="left">
            <input name="RepairLeadDays" type="text" value="@ViewData["RepairLeadDays"].ToString()"
                   id="RepairLeadDays" style="width: 50px;"/>
        </td>
        <td style="width: 62%" align="right">
        </td>
    </tr>
</table>
<table style="width: 100%">
    <tr>
        <td style="font-size: small; font-weight: 700; width: 28%;" align="right">
            <strong>Life Time Buy:&nbsp;</strong>
        </td>
        <td style="font-size: small; font-weight: 700; width: 15%;" align="left">
            <div id="divLTBDate">
                <input name="LTBDate" type="text" id="LTBDate" value="@ViewData["LTBDate"].ToString()"
                       onchange="javascript:GetViewStateLTB();" style="width: 80px;"/>
            </div>
        </td>
        <td style="font-size: small; font-weight: 700; width: 15%;" align="right">
            <strong style="text-align: right">End of Service:&nbsp;</strong>
        </td>
        <td style="font-size: small; font-weight: 700; width: 15%;" align="left">
            <div id="divEOSDate">
                <input name="EOSDate" type="text" id="EOSDate" value="@ViewData["EOSDate"].ToString()"
                       onchange="javascript:GetViewStateEOS();" style="width: 80px;"/>
            </div>
        </td>
        <td style="width: 27%" align="left">
        </td>
    </tr>
    <tr>
        <td class="AlignRight">
            <strong>
                @Html.Raw("Reparation möjlig?&nbsp;")</strong>
        </td>
        <td colspan="3">
            <div id="UpdatePanel59">
                <table id="rblRepair" border="0">
                    <tr>
                        <td>
                            <input id="rblRepair_0" type="radio" name="rblRepair" value="Repair" @Html.Raw(ViewData["RepairChecked"].ToString())
                                   onclick="@ViewData["Repair"].ToString()"/><label for="rblRepair_0">
                                <strong>@Html.Raw("Ja")</strong>
                            </label>
                        </td>
                        <td>
                            <input id="rblRepair_1" type="radio" name="rblRepair" value="No Repair" @Html.Raw(ViewData["NoRepairChecked"].ToString())
                                   onclick="@ViewData["NoRepair"].ToString()"/><label for="rblRepair_1">
                                <strong>@Html.Raw("Nej")</strong>
                            </label>
                        </td>

                    </tr>
                </table>
            </div>
        </td>
    </tr>
</table>
